<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".view.kullanici.KullaniciActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <fragment
            android:id="@+id/nav_host_fragment_kullanici_ac"
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:defaultNavHost="true"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/nav_view_kullanici_ac"
            app:navGraph="@navigation/nav_giris_graph"
            tools:ignore="InvalidId" />

        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:id="@+id/nav_view_kullanici_ac"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            app:itemIconTint="#000000"
            app:itemTextColor="#000000"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:menu="@menu/alt_gecis_menu" />

    </RelativeLayout>

</layout>